{
    "name": "popcorn4dinner/collections",
    "description": "A flexible, simple to use and non-opinionated collection library",
    "type": "library",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "esky/enum": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~6",
        "fzaninotto/faker": "^1.5.0"
    },
    "autoload": {
        "psr-4": {
            "Popcorn4dinner\\Collection": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Popcorn4dinner\\Collection\\Examples\\": "examples/",
            "Popcorn4dinner\\Collection\\Tests\\": "tests/unit/"
        }
    }
}
